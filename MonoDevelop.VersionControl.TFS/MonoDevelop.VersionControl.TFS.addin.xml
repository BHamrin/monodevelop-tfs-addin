<Addin id          = "VersionControl.TFS"
       namespace   = "MonoDevelop"
       name        = "TFS support"
       author      = "Ventsislav Mladenov"
       copyright   = "MIT X11"
       url         = "https://github.com/Indomitable/monodevelop-tfs-addin"
       description = "TFS support for the Version Control Add-in"
       category    = "Version Control"
       version     = "4.1.11">
 
	<Dependencies>
	    <Addin id="Core" version="4.1.11"/>
	    <Addin id="Ide" version="4.1.11"/>
	    <Addin id="VersionControl" version="4.1.11"/>
	</Dependencies>

	<Extension path="/MonoDevelop/VersionControl/VersionControlSystems">
		<Class class="MonoDevelop.VersionControl.TFS.TFSClient"/>
	</Extension>

	<Extension path = "/MonoDevelop/Ide/Commands/VersionControl">
		<Command id = "MonoDevelop.VersionControl.TFS.Commands.TFSCommands.SourceControlExplorer"
			_label = "View Source Control Exporer"
			defaultHandler = "MonoDevelop.VersionControl.TFS.Commands.SourceControlExplorerCommandHandler"
			description = "View Source Control Exporer"/>
	</Extension>
</Addin>
